    "demo": "node demo.js",
    "cli": "node packages/vend-cli/src/cli.js"
  },
  "keywords": [
    "vend",
    "blockchain",
    "ethereum",
    "alchemy",
    "x402",
    "api",
    "nft",
    "tokens",
    "erc20",
    "erc721",
    "data-vending",
    "payments",
    "usdc",
    "base",
    "web3",
    "micropayments",
    "transaction-history",
    "pay-per-use",
    "webhooks",
    "on-chain-verification"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "alchemy-sdk": "^3.4.8",
    "x402-express": "^0.1.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
